{
  "name": "mobile-app",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev-client": "expo start --dev-client",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "android:dev": "expo run:android --variant debug",
    "android:release": "expo run:android --variant release",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios --configuration Debug",
    "ios:release": "expo run:ios --configuration Release",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:dev": "NODE_ENV=development expo prebuild",
    "prebuild:preview": "NODE_ENV=staging expo prebuild",
    "prebuild:production": "NODE_ENV=production expo prebuild",
    "postinstall": "npx patch-package",
    "postinstall:dev": "echo 'Development postinstall complete'",
    "postinstall:preview": "echo 'Preview postinstall complete'",
    "postinstall:production": "echo 'Production postinstall complete'",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "clean": "expo r -c && rm -rf node_modules/.cache && rm -rf .expo && rm -rf dist && rm -rf builds",
    "clean:deep": "npm run clean && rm -rf node_modules && npm install",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "reset": "npm run clean:deep && npx expo install --fix",
    "analyze:bundle": "npx webpack-bundle-analyzer metro-cache --port 8888",
    "analyze:deps": "npx madge --circular --extensions ts,tsx,js,jsx src/",
    "analyze:size": "npx cost-of-modules",
    "perf:profile": "npx react-native-bundle-visualizer",
    "perf:memory": "node --expose-gc --inspect=9229 node_modules/.bin/jest --runInBand",
    "doctor": "expo doctor",
    "env:info": "expo config --type public",
    "prepare": "husky install || true",
    "validate": "npm run type-check && npm run lint && npm run test:ci"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@prisma/client": "^5.1.1",
    "@tanstack/react-query": "^4.36.0",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "axios": "^1.7.9",
    "eas-cli": "^16.13.2",
    "expo": "~51.0.0",
    "expo-av": "~14.0.0",
    "expo-camera": "~15.0.0",
    "expo-constants": "~16.0.0",
    "expo-dev-client": "~4.0.29",
    "expo-device": "~6.0.0",
    "expo-file-system": "~17.0.0",
    "expo-font": "~12.0.0",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.0",
    "expo-linking": "~6.3.0",
    "expo-location": "~17.0.0",
    "expo-notifications": "~0.28.0",
    "expo-router": "~3.5.0",
    "expo-secure-store": "~13.0.0",
    "expo-splash-screen": "~0.27.0",
    "expo-status-bar": "~1.12.0",
    "expo-system-ui": "~3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.0",
    "stripe": "^12.18.0",
    "zod": "^3.22.2",
    "zustand": "^4.4.1",
    "expo-build-properties": "~0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
