{
  "cli": {
    "version": ">= 5.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "workingDirectory": "./mobile-app"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "workingDirectory": "./mobile-app"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "workingDirectory": "./mobile-app"
    }
  },
  "submit": {
    "production": {}
  }
}